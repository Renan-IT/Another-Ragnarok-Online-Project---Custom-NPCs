				close;
		}
	}
	OnWarp:
		dispbottom "Vous allez être warp";
		if (comodo == 1){
			set alreadytalk, 0;
			warp "comodo",0,0;
		}
		else if (izlude == 1){
			set alreadytalk, 0;
			warp "izlude",0,0;
		}
		else if (prontera == 1){
			set alreadytalk, 0;
			warp "prontera",0,0;
		}
		else if (umbala == 1){
			set alreadytalk, 0;
			warp "umbala",0,0;
		}
	end;

	OnSec:
		dispbottom "Encore 5 secondes avant d'être téléporté, préparez-vous !";
	end;
	
