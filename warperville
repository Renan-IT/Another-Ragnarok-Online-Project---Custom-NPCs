//NPC Warper Alerbta : Payon, Izlude, Jawaii, (Nifheim via Port)
//NPC Warper Alde Baran : Yuno, Prontera, Einbech, Einbroch
//NPC Warper Amatsu : Gonryun, Nifheim, Jawaii
//NPC Warper Ayothaya : Gonryun, Lutie, (Louyang, Hugel via Port)
//NPC Warper Comodo : Morocc, (Alberta, Rachel, Veins, Izlude via port), Umbala
//NPC Warper Einbech : Einbroch via Train, Rachel, Alde Baran
//NPC Warper Einbroch : Einbech via Train, Rachel, (Yuno via Airship)
//NPC Warper Geffen : Prontera, Izlude, 
//NPC Warper Gonryun : Amatsu, Ayothaya, Louyang, Lutie, (Izlude via Port)
//NPC Warper Hugel : (Ayothaya via Port), Yuno, Alde Baran 
//NPC Warper Izlude : Geffen, Morocc, Payon, Prontera, (Yuno, Rachel, Hugel, Lighthalzen via Airship)
//NPC Warper Jawaii : Alberta, Amatsu, Nifheim
//NPC Warper Lighthalzen : Veins, Einbech, Einbroch, (Yuno, Rachel, Izlude, Hugel via Airhsip)
//NPC Warper Louyang : Lutie, (Gonryun, Ayothaya, Amatsu via Port)
//NPC Warper Lutie : Ayothaya, Gonryun, Louyang
//NPC Warper Morocc : Izlude, Prontera, Comodo, Umbala
//NPC Warper Nifheim : Amatsu, Jawaii, (Alberta via Port)
//NPC Warper Prontera : Alde Baran, Geffen, Izlude, Morocc, Payon
//NPC Warper Payon : Izlude, Prontera, Alberta
//NPC Warper Rachel : Veins, Lighthalzen, Einbroch, Einbech, (Yuno, Izlude, Lighthalzen, Hugel via Airship)
//NPC Warper Umbala : Morocc, Comodo
//NPC Warper Veins : Rachel, Einbech, Einbroch, Lighthalzen
//NPC Warper Yuno : Einbech, Einbroch, (Hugel, Izlude, Rachel, Lighthalzen via Airship)

//////////////////////////////////////////////////////////////////////////////////////////////////////
/////////////////////////////////////////////=NPC WARPER=/////////////////////////////////////////////
//////////////////////////////////////////////////////////////////////////////////////////////////////

//=NPC WARPER ALBERTA=//

alberta,100,100,4	script	Warper	453,{
set @npc$, "[Warper]"; //Nom NPC

	if (alreadytalk ==1){
	mes @npc$;
	mes "Nous préparons votre monture !";
	close;
	}
	else{
		mes @npc$;
		mes "Bonjour "+strcharinfo(0)+",";
		mes "Je peux vous transporter à dos de Peco Peco dans les villes alentours si vous le souhaitez.";
		mes "Les villes accéssibles ici sont : Prontera, Morocc, Geffen et Payon";
		next;
		mes @npc$;
		mes "Dans quelles villes souhaitez-vous aller ?";
		switch(select("Izlude","Jawaii","Payon","Aucune")){
			case 1:
				next;
				mes @npc$;
				mes "très bien je demande tout suite à un ecuyer de vous préparer votre Peco Peco !";
				mes "Il sera prêt dans 20s environ,";
				mes "pendant ce temps là, prenez le temps de préparer 	votre équipement !";
				addtimer 20000, strnpcinfo(3)+"::OnWarp";
				addtimer 15000, strnpcinfo(3)+"::OnSec";
				set izlude, 1;
				//warp après 20s
				set alreadytalk, 1;
				close;
			case 2:
				next;
				mes @npc$;
				mes "très bien je demande tout suite à un ecuyer de vous préparer votre Peco Peco !";
				mes "Il sera prêt dans 20s environ,";
				mes "pendant ce temps là, prenez le temps de préparer votre équipement !";
				addtimer 20000, strnpcinfo(3)+"::OnWarp";
				addtimer 15000, strnpcinfo(3)+"::OnSec";
				set jawaii, 1;
				//warp après 20s
				set alreadytalk, 1;
				close;
			case 3:
				next;
				mes @npc$;
				mes "très bien je demande tout suite à un ecuyer de vous 	préparer votre Peco Peco !";
				mes "Il sera prêt dans 20s environ,";
				mes "pendant ce temps là, prenez le temps de préparer 	votre équipement !";
				addtimer 20000, strnpcinfo(3)+"::OnWarp";
				addtimer 15000, strnpcinfo(3)+"::OnSec";
				set payon, 1;
				//warp après 20s
				set alreadytalk, 1;
				close;
			case 4:
				mes @npc$;
				mes "A bientôt j'espère !";
				close;
		}
	}
	OnWarp:
		dispbottom "Vous allez être warp";
		if (izlude == 1){
			set alreadytalk, 0;
			warp "izlude",0,0;
		}
		else if (jawaii == 1){
			set alreadytalk, 0;
			warp "jawaii",0,0;
		}
		else if (payon == 1){
			set alreadytalk, 0;
			warp "payon",0,0;
		}
	end;

	OnSec:
		dispbottom "Encore 5 secondes avant d'être téléporté, préparez-vous !";
	end;
	
}

//=NPC WARPER ALDE BARAN=//

aldebaran,100,100,4	script	Warper	453,{
set @npc$, "[Warper]"; //Nom NPC

	if (alreadytalk ==1){
	mes @npc$;
	mes "Nous préparons votre monture !";
	close;
	}
	else{
		mes @npc$;
		mes "Bonjour "+strcharinfo(0)+",";
		mes "Je peux vous transporter à dos de Peco Peco dans les villes alentours si vous le souhaitez.";
		mes "Les villes accéssibles ici sont : Prontera, Morocc, Geffen et Payon";
		next;
		mes @npc$;
		mes "Dans quelles villes souhaitez-vous aller ?";
		switch(select("Einbech","Einbroch","Prontera","Yuno","Aucune")){
			case 1:
				next;
				mes @npc$;
				mes "très bien je demande tout suite à un ecuyer de vous préparer votre Peco Peco !";
				mes "Il sera prêt dans 20s environ,";
				mes "pendant ce temps là, prenez le temps de préparer 	votre équipement !";
				addtimer 20000, strnpcinfo(3)+"::OnWarp";
				addtimer 15000, strnpcinfo(3)+"::OnSec";
				set einbech, 1;
				//warp après 20s
				set alreadytalk, 1;
				close;
			case 2:
				next;
				mes @npc$;
				mes "très bien je demande tout suite à un ecuyer de vous préparer votre Peco Peco !";
				mes "Il sera prêt dans 20s environ,";
				mes "pendant ce temps là, prenez le temps de préparer votre équipement !";
				addtimer 20000, strnpcinfo(3)+"::OnWarp";
				addtimer 15000, strnpcinfo(3)+"::OnSec";
				set einbroch, 1;
				//warp après 20s
				set alreadytalk, 1;
				close;
			case 3:
				next;
				mes @npc$;
				mes "très bien je demande tout suite à un ecuyer de vous 	préparer votre Peco Peco !";
				mes "Il sera prêt dans 20s environ,";
				mes "pendant ce temps là, prenez le temps de préparer 	votre équipement !";
				addtimer 20000, strnpcinfo(3)+"::OnWarp";
				addtimer 15000, strnpcinfo(3)+"::OnSec";
				set prontera, 1;
				//warp après 20s
				set alreadytalk, 1;
				close;
			case 4:
				next;
				mes @npc$;
				mes "très bien je demande tout suite à un ecuyer de vous préparer votre Peco Peco !";
				mes "Il sera prêt dans 20s environ,";
				mes "pendant ce temps là, prenez le temps de préparer votre équipement !";
				addtimer 20000, strnpcinfo(3)+"::OnWarp";
				addtimer 15000, strnpcinfo(3)+"::OnSec";
				set yuno, 1;
				//warp après 20s
				set alreadytalk, 1;
				close;
			case 5:
				mes @npc$;
				mes "A bientôt j'espère !";
				close;
		}
	}
	OnWarp:
		dispbottom "Vous allez être warp";
		if (einbech == 1){
			set alreadytalk, 0;
			warp "einbech",0,0;
		}
		else if (einbroch == 1){
			set alreadytalk, 0;
			warp "einbroch",0,0;
		}
		else if (prontera == 1){
			set alreadytalk, 0;
			warp "prontera",0,0;
		}
		else if (yuno == 1){
			set alreadytalk, 0;
			warp "yuno",0,0;
		}
	end;

	OnSec:
		dispbottom "Encore 5 secondes avant d'être téléporté, préparez-vous !";
	end;
	
}


//=NPC WARPER AMATSU=//

amatsu,100,100,4	script	Warper	453,{
set @npc$, "[Warper]"; //Nom NPC

	if (alreadytalk ==1){
	mes @npc$;
	mes "Nous préparons votre monture !";
	close;
	}
	else{
		mes @npc$;
		mes "Bonjour "+strcharinfo(0)+",";
		mes "Je peux vous transporter à dos de Peco Peco dans les villes alentours si vous le souhaitez.";
		mes "Les villes accéssibles ici sont : Prontera, Morocc, Geffen et Payon";
		next;
		mes @npc$;
		mes "Dans quelles villes souhaitez-vous aller ?";
		switch(select("Gonryun","Jawaii","Nifheim","Aucune")){
			case 1:
				next;
				mes @npc$;
				mes "très bien je demande tout suite à un ecuyer de vous préparer votre Peco Peco !";
				mes "Il sera prêt dans 20s environ,";
				mes "pendant ce temps là, prenez le temps de préparer 	votre équipement !";
				addtimer 20000, strnpcinfo(3)+"::OnWarp";
				addtimer 15000, strnpcinfo(3)+"::OnSec";
				set gonryun, 1;
				//warp après 20s
				set alreadytalk, 1;
				close;
			case 2:
				next;
				mes @npc$;
				mes "très bien je demande tout suite à un ecuyer de vous préparer votre Peco Peco !";
				mes "Il sera prêt dans 20s environ,";
				mes "pendant ce temps là, prenez le temps de préparer votre équipement !";
				addtimer 20000, strnpcinfo(3)+"::OnWarp";
				addtimer 15000, strnpcinfo(3)+"::OnSec";
				set jawaii, 1;
				//warp après 20s
				set alreadytalk, 1;
				close;
			case 3:
				next;
				mes @npc$;
				mes "très bien je demande tout suite à un ecuyer de vous 	préparer votre Peco Peco !";
				mes "Il sera prêt dans 20s environ,";
				mes "pendant ce temps là, prenez le temps de préparer 	votre équipement !";
				addtimer 20000, strnpcinfo(3)+"::OnWarp";
				addtimer 15000, strnpcinfo(3)+"::OnSec";
				set nifheim, 1;
				//warp après 20s
				set alreadytalk, 1;
				close;
			case 4:
				mes @npc$;
				mes "A bientôt j'espère !";
				close;
		}
	}
	OnWarp:
		dispbottom "Vous allez être warp";
		if (gonryun == 1){
			set alreadytalk, 0;
			warp "gonryun",0,0;
		}
		else if (jawaii == 1){
			set alreadytalk, 0;
			warp "jawaii",0,0;
		}
		else if (nifheim == 1){
			set alreadytalk, 0;
			warp "nifheim",0,0;
		}
	end;

	OnSec:
		dispbottom "Encore 5 secondes avant d'être téléporté, préparez-vous !";
	end;
	
}

